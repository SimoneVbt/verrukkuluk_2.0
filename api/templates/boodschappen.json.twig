[
{% for bs in boodschappen %}
    {
        "id": {{ bs.id }},
        "artikel_id": {{ bs.artikelId }},
        "product": "{{ bs.product|e('js')|raw }}",
        "eenheid": "{{ bs.eenheid }}",
        "verpakking": {{ bs.verpakking }},
        "prijs": {{ bs.prijs|number_format(2) }},
        "aantal": {{ bs.aantal }},
        "aantal_verpakkingen": {{ bs.aantal_verpakkingen }},
        "totale_prijs": {{ bs.totalePrijs|number_format(2) }}
    {{ bs == boodschappen|last ? "}" : "}," }}
{% endfor %}
]
