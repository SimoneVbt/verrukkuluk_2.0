[
    {% for dish in dishes %}
    {
        "id": {{ dish.id }},
        "keuken": "{{ dish.keuken }}",
        "type": "{{ dish.type }}",
        "gebruikerId": {{ dish.gebruikerId }},
        "gebruiker": "{{ dish.gebruiker }}",
        "afbeelding": "{{ dish.afbeelding }}",
        "datum_toegevoegd": "{{ dish.datumToegevoegd|date("Y-m-d") }}",
        "titel": "{{ dish.titel }}",
        "korte_omschrijving": "{{ dish.korteOmschrijving }}",
        "lange_omschrijving": "{{ dish.langeOmschrijving }}",
        "gemiddelde_beoordeling": {{ dish.gemiddeldeBeoordeling }},
        "calorieen": {{ dish.calorieen|number_format(0) }},
        "totale_prijs": "{{ dish.totalePrijs|number_format(2, ',', '.') }}"
    {{ dish == dishes|last ? "}" : "}," }}
    {% endfor %}
]