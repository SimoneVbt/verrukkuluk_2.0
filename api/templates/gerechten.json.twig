[
    {% for dish in dishes %}
    {
        "id": {{ dish.id }},
        "keuken": "{{ dish.keuken|e('js')|raw }}",
        "keuken_id": {{ dish.keukenId }},
        "type": "{{ dish.type|e('js')|raw }}",
        "type_id": {{ dish.typeId }},
        "gebruiker_id": {{ dish.gebruikerId }},
        "gebruiker": "{{ dish.gebruiker|e('js')|raw }}",
        "afbeelding": "{{ dish.afbeelding }}",
        "datum_toegevoegd": "{{ dish.datumToegevoegd|date("Y-m-d") }}",
        "titel": "{{ dish.titel }}",
        "korte_omschrijving": "{{ dish.korteOmschrijving|e('js')|raw }}",
        "lange_omschrijving": "{{ dish.langeOmschrijving|e('js')|raw }}",
        "ingr_set": {{ dish.ingrSet ? "true" : "false" }},
        "bereiding_set": {{ dish.bereidingSet ? "true" : "false" }},
        "complete": {{ dish.complete ? "true" : "false" }},
        "gemiddelde_beoordeling": {{ dish.gemiddeldeBeoordeling }},
        "calorieen": {{ dish.calorieen|number_format(0) }},
        "totale_prijs": "{{ dish.totalePrijs|number_format(2, ',', '.') }}",
        "favoriet_id": {{ dish.favoriet  ? dish.favoriet.id : "0" }},
        "favoriet": {{ dish.favoriet  ? "true" : "false" }},
        "waardering_id": {{ dish.waardering  ? dish.waardering.id : "0" }},
        "waardering": {{ dish.waardering ? dish.waardering.nummeriekveld : "0" }}
    {{ dish == dishes|last ? "}" : "}," }}
    {% endfor %}
]